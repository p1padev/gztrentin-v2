---
interface Props {
  title: string;
}

const {title} = Astro.props as Props;
---

<li class="flex items-center gap-4 px-4 py-3 ">
  <div class="text-center flex-1 min-h-[2rem] border-b-[1px] border-solid border-neutral-200">
    <h4 class="text-base text-slate-700 font-medium">{title}</h4>
  </div>
</li>